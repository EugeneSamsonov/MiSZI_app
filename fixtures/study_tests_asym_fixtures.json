  { "model": "study_tests.question", "pk": 32, "fields": { "test": 4, "text": "Какие требования к k в ElGamal?", "multiple_answers": false } },
  { "model": "study_tests.answer", "pk": 125, "fields": { "question": 32, "text": "k выбирается случайно каждый раз", "is_correct": true } },
  { "model": "study_tests.answer", "pk": 126, "fields": { "question": 32, "text": "gcd(k, p−1) = 1", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 127, "fields": { "question": 32, "text": "k = 1 для повторяемости", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 128, "fields": { "question": 32, "text": "k должен делить p−1", "is_correct": false } },

  { "model": "study_tests.question", "pk": 33, "fields": { "test": 4, "text": "Что входит в открытый ключ ElGamal?", "multiple_answers": false } },
  { "model": "study_tests.answer", "pk": 129, "fields": { "question": 33, "text": "(p, g, y)", "is_correct": true } },
  { "model": "study_tests.answer", "pk": 130, "fields": { "question": 33, "text": "Секретное x", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 131, "fields": { "question": 33, "text": "Только g", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 132, "fields": { "question": 33, "text": "(p, q, d)", "is_correct": false } },

  { "model": "study_tests.question", "pk": 34, "fields": { "test": 4, "text": "Выберите формулу расшифрования ElGamal", "multiple_answers": false } },
  { "model": "study_tests.answer", "pk": 133, "fields": { "question": 34, "text": "M = b * a^(p−1−x) mod p", "is_correct": true } },
  { "model": "study_tests.answer", "pk": 134, "fields": { "question": 34, "text": "Эквивалентно b * (a^x)⁻¹ mod p", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 135, "fields": { "question": 34, "text": "M = b − a^x", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 136, "fields": { "question": 34, "text": "M = a / b mod p", "is_correct": false } },

  { "model": "study_tests.question", "pk": 35, "fields": { "test": 4, "text": "Что верно про подпись ElGamal?", "multiple_answers": false } },
  { "model": "study_tests.answer", "pk": 137, "fields": { "question": 35, "text": "Подпись — пара (r, s)", "is_correct": true } },
