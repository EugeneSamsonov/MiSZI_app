  { "model": "study_tests.answer", "pk": 139, "fields": { "question": 35, "text": "Использует S-box", "is_correct": false } },
  { "model": "study_tests.answer", "pk": 140, "fields": { "question": 35, "text": "r = k mod p без возведения", "is_correct": false } },

