  { "model": "study_tests.question", "pk": 31, "fields": { "test": 4, "text": "Что верно про ElGamal-шифрование?", "multiple_answers": false } },
  { "model": "study_tests.answer", "pk": 121, "fields": { "question": 31, "text": "Работает в мультипликативной группе mod p", "is_correct": true } },
